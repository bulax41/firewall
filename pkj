*mangle
:PREROUTING ACCEPT [0:0]
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:POSTROUTING ACCEPT [0:0]
COMMIT
*nat
:PREROUTING ACCEPT [0:0]
:POSTROUTING ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
-A POSTROUTING -o gre+ -j ACCEPT
-A POSTROUTING -o tun+ -j ACCEPT
COMMIT
*filter
:INPUT DROP [0:0]
:FORWARD DROP [0:0]
:OUTPUT DROP [0:0]
:DROPnLOG - [0:0]
:INBOUND - [0:0]
:IPSEC_PEERS - [0:0]
:OUTBOUND - [0:0]
:OUTPUT_ETH0 - [0:0]
:OUTPUT_ETH1 - [0:0]
:OUTPUT_ETH2 - [0:0]
:OUTPUT_GRE - [0:0]
-A INPUT -m state --state RELATED -j ACCEPT
-A INPUT -m state --state ESTABLISHED -j ACCEPT
-A INPUT -m state --state INVALID -j DROPnLOG
-A INPUT -i lo -j ACCEPT
-A INPUT -p icmp -j ACCEPT
-A INPUT -p udp -m udp --dport 1194 -j ACCEPT
-A INPUT -p tcp --dport 2222 -j ACCEPT
-A INPUT  -p esp -j IPSEC_PEERS
-A INPUT -p udp -m udp --dport 500 -j IPSEC_PEERS
-A INPUT  -p udp -m udp --dport 4500 -j IPSEC_PEERS
-A INPUT -j DROPnLOG
-A FORWARD -o ens224 -j INBOUND
-A FORWARD -o ens192 -j OUTBOUND
-A FORWARD -o tun+ -m state --state RELATED,ESTABLISHED -j ACCEPT
-A FORWARD -j DROPnLOG
-A OUTPUT -m state --state RELATED -j ACCEPT
-A OUTPUT -m state --state ESTABLISHED -j ACCEPT
-A OUTPUT -o lo -j ACCEPT
-A OUTPUT -p icmp -j ACCEPT
-A OUTPUT -j DROPnLOG
-A DROPnLOG -m limit --limit 10/sec -j LOG
-A DROPnLOG -j DROP
